# Example environment variables for local development
# Copy this file to .env and fill in your values

# Yandex Cloud
YC_TOKEN=your-yandex-cloud-token
CLOUD_ID=your-cloud-id
FOLDER_ID=your-folder-id

# YDB
YDB_ENDPOINT=grpcs://ydb.serverless.yandexcloud.net:2135
YDB_DATABASE=/ru-central1/your-database-id/your-database-name

# Message Queue
MQ_QUEUE_URL=https://message-queue.api.cloud.yandex.net/your-queue-url

# Object Storage
S3_BUCKET=your-bucket-name
S3_ENDPOINT=https://storage.yandexcloud.net

# API Keys (stored in Lockbox in production)
SPEECHKIT_API_KEY=your-speechkit-api-key
YANDEXGPT_API_KEY=your-yandexgpt-api-key
